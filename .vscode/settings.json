{
    "files.exclude": {
        "**/*.3db": true,
        "**/*.ale": true,
        "**/*.cmp": true,
        "**/*.txm": true,
        "**/*.utf": true,
        "**/*.wav": true,
        "**/*.mat": true,
        "**/*.anm": true,
        "**/*.dfm": true,
        "**/.thorn-cache": true,
        "**/.bini-cache": true,
        "**/__pycache__":true,
        "**/__init__.py":true,
        "**/csv_dump":true,
        "**/*.~lock*": true
    },
    "editor.wordWrap": "off",
    "files.associations": {
        "*.thn": "lua",
        "*.fl": "ini"
    }
}